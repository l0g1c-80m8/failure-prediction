<mujoco model="ur5e scene">
  <compiler angle="radian" meshdir="assets" autolimits="true"/>

  <option integrator="implicitfast" gravity="0 0 -9.81"/>

  <default>
    <default class="ur5e">
      <material specular="0.5" shininess="0.25"/>
      <joint axis="0 1 0" range="-6.28319 6.28319" armature="0.1"/>
      <general gaintype="fixed" biastype="affine" ctrlrange="-6.2831 6.2831" gainprm="2000" biasprm="0 -2000 -400"
        forcerange="-150 150"/>
      <default class="size3">
        <default class="size3_limited">
          <joint range="-3.1415 3.1415"/>
          <general ctrlrange="-3.1415 3.1415"/>
        </default>
      </default>
      <default class="size1">
        <general gainprm="500" biasprm="0 -500 -100" forcerange="-28 28"/>
      </default>
      <default class="visual">
        <geom type="mesh" contype="0" conaffinity="0" group="0"/>
      </default>
      <default class="collision">
        <geom type="capsule" group="0"/>
        <default class="eef_collision">
          <geom type="cylinder" group="0"/>
        </default>
      </default>
      <site size="0.001" rgba="0.5 0.5 0.5 0.3" group="0"/>
    </default>
  </default>

  <asset>
    <material class="ur5e" name="black" rgba="0.033 0.033 0.033 1"/>
    <material class="ur5e" name="jointgray" rgba="0.278 0.278 0.278 1"/>
    <material class="ur5e" name="linkgray" rgba="0.82 0.82 0.82 1"/>
    <material class="ur5e" name="urblue" rgba="0.49 0.678 0.8 1"/>

    <mesh file="base_0.obj"/>
    <mesh file="base_1.obj"/>
    <mesh file="shoulder_0.obj"/>
    <mesh file="shoulder_1.obj"/>
    <mesh file="shoulder_2.obj"/>
    <mesh file="upperarm_0.obj"/>
    <mesh file="upperarm_1.obj"/>
    <mesh file="upperarm_2.obj"/>
    <mesh file="upperarm_3.obj"/>
    <mesh file="forearm_0.obj"/>
    <mesh file="forearm_1.obj"/>
    <mesh file="forearm_2.obj"/>
    <mesh file="forearm_3.obj"/>
    <mesh file="wrist1_0.obj"/>
    <mesh file="wrist1_1.obj"/>
    <mesh file="wrist1_2.obj"/>
    <mesh file="wrist2_0.obj"/>
    <mesh file="wrist2_1.obj"/>
    <mesh file="wrist2_2.obj"/>
    <mesh file="wrist3.obj"/>
  </asset>

  <contact>
    <pair geom1="sliding_cube" geom2="panel" friction="0.2 0.5 0.001"/>
  </contact>

  <worldbody>
    <!-- Add camera definition -->
    <camera name="top_camera" mode="fixed" pos="0 0 3.0" euler="0 0 0"  fovy="45"/>
    <!-- <camera name="side_camera" mode="fixed" pos="1.5 0 0.8" euler="0 -30 0" fovy="45"/> -->
    <camera name="front_camera" mode="fixed" pos="3.0 0 2" euler="0 45 0" fovy="45"/>
    <!-- <light name="spotlight" mode="targetbodycom" target="wrist_2_link" pos="0 -1 2"/> -->
    <body name="base" quat="1 0 0 1" childclass="ur5e">
      <inertial mass="4.0" pos="0 0 0" diaginertia="0.00443333156 0.00443333156 0.0072"/>
      <geom mesh="base_0" material="black" class="visual"/>
      <geom mesh="base_1" material="jointgray" class="visual"/>
      <body name="shoulder_link" pos="0 0 0.163">
        <inertial mass="3.7" pos="0 0 0" diaginertia="0.0102675 0.0102675 0.00666"/>
        <joint name="shoulder_pan_joint" class="size3" axis="0 0 1"/>
        <geom mesh="shoulder_0" material="urblue" class="visual"/>
        <geom mesh="shoulder_1" material="black" class="visual"/>
        <geom mesh="shoulder_2" material="jointgray" class="visual"/>
        <geom class="collision" size="0.06 0.06" pos="0 0 -0.04"/>
        <body name="upper_arm_link" pos="0 0.138 0" quat="1 0 1 0">
          <inertial mass="8.393" pos="0 0 0.2125" diaginertia="0.133886 0.133886 0.0151074"/>
          <joint name="shoulder_lift_joint" class="size3"/>
          <geom mesh="upperarm_0" material="linkgray" class="visual"/>
          <geom mesh="upperarm_1" material="black" class="visual"/>
          <geom mesh="upperarm_2" material="jointgray" class="visual"/>
          <geom mesh="upperarm_3" material="urblue" class="visual"/>
          <geom class="collision" pos="0 -0.04 0" quat="1 1 0 0" size="0.06 0.06"/>
          <geom class="collision" size="0.05 0.2" pos="0 0 0.2"/>
          <body name="forearm_link" pos="0 -0.131 0.425">
            <inertial mass="2.275" pos="0 0 0.196" diaginertia="0.0311796 0.0311796 0.004095"/>
            <joint name="elbow_joint" class="size3_limited"/>
            <geom mesh="forearm_0" material="urblue" class="visual"/>
            <geom mesh="forearm_1" material="linkgray" class="visual"/>
            <geom mesh="forearm_2" material="black" class="visual"/>
            <geom mesh="forearm_3" material="jointgray" class="visual"/>
            <geom class="collision" pos="0 0.08 0" quat="1 1 0 0" size="0.055 0.06"/>
            <geom class="collision" size="0.038 0.19" pos="0 0 0.2"/>
            <body name="wrist_1_link" pos="0 0 0.392" quat="1 0 1 0">
              <inertial mass="1.219" pos="0 0.127 0" diaginertia="0.0025599 0.0025599 0.0021942"/>
              <joint name="wrist_1_joint" class="size1"/>
              <geom mesh="wrist1_0" material="black" class="visual"/>
              <geom mesh="wrist1_1" material="urblue" class="visual"/>
              <geom mesh="wrist1_2" material="jointgray" class="visual"/>
              <geom class="collision" pos="0 0.05 0" quat="1 1 0 0" size="0.04 0.07"/>
              <body name="wrist_2_link" pos="0 0.127 0">
                <inertial mass="1.219" pos="0 0 0.1" diaginertia="0.0025599 0.0025599 0.0021942"/>
                <joint name="wrist_2_joint" axis="0 0 1" class="size1"/>
                <geom mesh="wrist2_0" material="black" class="visual"/>
                <geom mesh="wrist2_1" material="urblue" class="visual"/>
                <geom mesh="wrist2_2" material="jointgray" class="visual"/>
                <geom class="collision" size="0.04 0.06" pos="0 0 0.04"/>
                <geom class="collision" pos="0 0.02 0.1" quat="1 1 0 0" size="0.04 0.04"/>
                <body name="wrist_3_link" pos="0 0 0.1">
                  <inertial mass="0.1889" pos="0 0.0771683 0" quat="1 0 0 1"
                    diaginertia="0.000132134 9.90863e-05 9.90863e-05"/>
                  <joint name="wrist_3_joint" class="size1"/>
                  <geom material="linkgray" mesh="wrist3" class="visual"/>
                  <geom class="eef_collision" pos="0 0.08 0" quat="1 1 0 0" size="0.04 0.02"/>
                  <site name="attachment_site" pos="0 0.1 0" quat="-1 1 0 0"/>
                  <body name="fixed_box" pos="0 0.25 0" quat="0 0 0 1">
                    <joint name="box_rotation" type="hinge" axis="0 1 0" limited="true" range="-0.1 0.1"/>
                    <geom name="panel" type="box" size="0.075 0.15 0.01" rgba="0.8 0.2 0.2 1" group="1"/> <!-- Box dimensions and color -->
                    <inertial mass="0.5" pos="0 0 0" diaginertia="0.001 0.001 0.001"/>
                  </body>
                </body>
              </body>
            </body>
          </body>
        </body>
      </body>
    </body>
                      
    <!-- New Cube Body -->
    <body name="free_cube" pos="-0.4 0.8 0.2"> <!-- Slightly above the box -->
      <!-- <joint name="free_cube" type="free" /> -->
      <geom name="sliding_cube" type="box" size="0.015 0.015 0.015" rgba="0.2 0.8 0.2 1" group="2"/> <!-- Cube dimensions and color -->
      <joint type="free"/>
      <inertial mass="1" pos="0 0 0" quat="1 0 0 0" diaginertia="0.01 0.01 0.01"/>
    </body>

    
                  
  </worldbody>

  <actuator>
    <general class="size3" name="shoulder_pan" joint="shoulder_pan_joint"/>
    <general class="size3" name="shoulder_lift" joint="shoulder_lift_joint"/>
    <general class="size3_limited" name="elbow" joint="elbow_joint"/>
    <general class="size1" name="wrist_1" joint="wrist_1_joint"/>
    <general class="size1" name="wrist_2" joint="wrist_2_joint"/>
    <general class="size1" name="wrist_3" joint="wrist_3_joint"/>
    <motor joint="box_rotation" name="box_rotation_motor" gear="1"/>
  </actuator>

  <keyframe>
    <key name="home" qpos="1.3500753657730578 -1.865456836594901 -2.0400916711745514 -2.3380169136860096 -0.26860617161064493 3.0982337160377313 0 0 0 1 0 0 0 0" 
          ctrl="1.3500753657730578 -1.865456836594901 -2.0400916711745514 -2.3380169136860096 -0.26860617161064493 3.0982337160377313 0"/>
  </keyframe>

  <statistic center="0.3 0 0.4" extent="0.8"/>

  <visual>
    <!-- <headlight diffuse="0.6 0.6 0.6" ambient="0.1 0.1 0.1" specular="0 0 0"/> -->
    <rgba haze="0.15 0.25 0.35 1"/>
    <global azimuth="120" elevation="-20"/>
  </visual>

  <asset>
    <texture type="skybox" builtin="gradient" rgb1="0.3 0.5 0.7" rgb2="0 0 0" width="512" height="3072"/>
    <!-- Define multiple floor textures -->
    <texture type="2d" name="groundplane1" builtin="gradient" mark="edge" rgb1="0.2 0.3 0.4" rgb2="0.1 0.2 0.3"
      markrgb="0.8 0.8 0.8" width="300" height="300"/>
    <texture type="2d" name="groundplane2" builtin="gradient" mark="cross" rgb1="0.4 0.2 0.3" rgb2="0.3 0.1 0.2"
      markrgb="0.8 0.8 0.8" width="300" height="300"/>
    <texture type="2d" name="groundplane3" builtin="gradient" mark="random" rgb1="0.3 0.4 0.2" rgb2="0.2 0.3 0.1"
      markrgb="0.8 0.8 0.8" width="300" height="300"/>
    <texture type="2d" name="groundplane4" builtin="checker" mark="edge" rgb1="0.2 0.3 0.4" rgb2="0.1 0.2 0.3"
      markrgb="0.8 0.8 0.8" width="300" height="300"/>
    <texture type="2d" name="groundplane5" builtin="checker" mark="cross" rgb1="0.4 0.2 0.3" rgb2="0.3 0.1 0.2"
      markrgb="0.8 0.8 0.8" width="300" height="300"/>
    <texture type="2d" name="groundplane6" builtin="checker" mark="random" rgb1="0.3 0.4 0.2" rgb2="0.2 0.3 0.1"
      markrgb="0.8 0.8 0.8" width="300" height="300"/>
    <texture type="2d" name="groundplane7" builtin="flat" mark="edge" rgb1="0.2 0.3 0.4" rgb2="0.1 0.2 0.3"
      markrgb="0.8 0.8 0.8" width="300" height="300"/>
    <texture type="2d" name="groundplane8" builtin="flat" mark="cross" rgb1="0.4 0.2 0.3" rgb2="0.3 0.1 0.2"
      markrgb="0.8 0.8 0.8" width="300" height="300"/>
    <texture type="2d" name="groundplane9" builtin="flat" mark="random" rgb1="0.3 0.4 0.2" rgb2="0.2 0.3 0.1"
      markrgb="0.8 0.8 0.8" width="300" height="300"/>
    
    <!-- Define materials for each texture -->
    <material name="groundplane1" texture="groundplane1" texuniform="true" texrepeat="5 5" reflectance="0.2"/>
    <material name="groundplane2" texture="groundplane2" texuniform="true" texrepeat="5 5" reflectance="0.2"/>
    <material name="groundplane3" texture="groundplane3" texuniform="true" texrepeat="5 5" reflectance="0.2"/>
    <material name="groundplane4" texture="groundplane4" texuniform="true" texrepeat="5 5" reflectance="0.2"/>
    <material name="groundplane5" texture="groundplane5" texuniform="true" texrepeat="5 5" reflectance="0.2"/>
    <material name="groundplane6" texture="groundplane6" texuniform="true" texrepeat="5 5" reflectance="0.2"/>
    <material name="groundplane7" texture="groundplane7" texuniform="true" texrepeat="5 5" reflectance="0.2"/>
    <material name="groundplane8" texture="groundplane8" texuniform="true" texrepeat="5 5" reflectance="0.2"/>
    <material name="groundplane9" texture="groundplane9" texuniform="true" texrepeat="5 5" reflectance="0.2"/>
  </asset>

  <worldbody>
    <!-- <light pos="0 0 1.5" dir="0 0 -1" directional="true"/> -->
    <geom name="floor" size="0 0 0.05" type="plane" material="groundplane1" group="0"/>
  </worldbody>
</mujoco>